<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>register</title>
    <link rel="stylesheet" href="css/lib/bootstrap.min.css">
</head>
<body>
    <div class="container bs-docs-container">
        <% include nav.ejs %>
        <div class="row">
            <form class="col-md-6">
                <div class="form-group">
                    <label for="name">Name</label>
                    <input type="text" class="form-control" id="name" placeholder="Name" minlength="6" required/>
                </div>
                <div class="form-group">
                    <label for="phone">Phone</label>
                    <input type="phone" class="form-control" id="phone" placeholder="phone" maxlength="11" minlength="11" required/>
                </div>
                <div class="form-group">
                    <label for="password">Password</label>
                    <input type="password" class="form-control" id="password" placeholder="Password" maxlength="6" minlength="6" required/>
                </div>
                <div class="alert alert-danger" role="alert" style="display:none;">
                </div>
                <div class="alert alert-info" role="alert" style="display:none;">
                </div>
                <button type="button" class="btn btn-default">Submit</button>
            </form>
            <div class="col-md-6"></div>
        </div>
    </div>
</body>
</html>
<script src="js/lib/jquery.min.js"></script>
<script src="js/register.js"></script>